{
  "name": "l3l0/invoice",
  "autoload": {
    "psr-4": {
      "L3l0Labs\\": "src/L3l0Labs/",
      "App\\": "src/App/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^7.1.3",
    "symfony/flex": "^1.0",
    "symfony/framework-bundle": "^3.3",
    "symfony/yaml": "^3.3",
    "symfony/console": "^3.3",
    "symfony/twig-bundle": "^3.3",
    "symfony/asset": "^3.3",
    "symfony/orm-pack": "^1.0",
    "symfony/profiler-pack": "^1.0",
    "symfony/security-bundle": "^3.3",
    "ramsey/uuid": "^3.6",
    "symfony/form": "^3.3",
    "symfony/validator": "^3.3"
  },
  "require-dev": {
    "symfony/dotenv": "^3.3",
    "behat/behat": "~3.3.1",
    "phpspec/phpspec": "~3.4.0",
    "phpunit/phpunit": "~6.1.4",
    "doctrine/doctrine-fixtures-bundle": "^2.3"
  },
  "config": {
    "bin-dir": "bin"
  },
  "scripts": {
    "auto-scripts": {
      "make cache-warmup": "script",
      "assets:install --symlink --relative %WEB_DIR%": "symfony-cmd"
    }
  }
}
